{
    "name": "tomatophp/tomato-dusk",
    "type": "library",
    "description": "Laravel Dusk unit test with GUI for Tomato Framework",
    "keywords": [
        "php",
        "laravel",
        "template",
        "laravel",
        "generator",
        "resource",
        "page",
        "widget",
        "pages",
        "modal",
        "admin one",
        "vuejs",
        "ineriajs",
        "tailwind.css",
        "dusk",
        "unittest",
        "splade",
        "GUI"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoDusk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoDusk\\TomatoDuskServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "tomatophp/console-helpers": "^1.0",
        "laravel/dusk": "^7.4"
    }
}
